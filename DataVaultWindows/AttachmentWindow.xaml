<Window x:Class="DataVaultWindows.AttachmentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataVaultWindows"
        mc:Ignorable="d"
        Title="AttachmentWindow" Height="800" Width="800">
    <Grid>
        <Image x:Name="Attachment_Image" Margin="10,70,10,10"/>
        <Button x:Name="Export_Button" Content="Export" Margin="0,16,22,0" Height="20" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75"/>
    </Grid>
</Window>
