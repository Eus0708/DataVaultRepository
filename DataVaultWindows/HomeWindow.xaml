<Window x:Class="DataVaultWindows.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataVaultWindows"
        mc:Ignorable="d"
        Title="HomeWindow" Height="550" Width="850" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Home_Window" Content="New Profile" HorizontalAlignment="Left" Margin="286,95,0,0" VerticalAlignment="Top" Width="274" Height="81" FontSize="20" Click="Main_Window_Button"/>
        <Button x:Name="Existing_Profile" Content="Existing Profile" HorizontalAlignment="Left" Margin="286,225,0,0" VerticalAlignment="Top" Width="274" Height="80" FontSize="20" Click="Existing_Profile_Click"/>
        <Button x:Name="Exit" Content="Exit" HorizontalAlignment="Left" Margin="286,353,0,0" VerticalAlignment="Top" Width="274" Height="80" FontSize="20" Click="Exit_Button"/>
        <Image HorizontalAlignment="Left" Height="49" Margin="331,370,0,0" VerticalAlignment="Top" Width="51" Source="Resources/Exit_Icon.png" RenderTransformOrigin="4.265,0.5"/>

    </Grid>
</Window>
